# Environment Configuration for InnArt NFT Platform

# Ethereum Network Configuration
VITE_NETWORK_ID=11155111
VITE_CHAIN_ID=11155111
VITE_NETWORK_NAME=sepolia

# Ethereum RPC Configuration
VITE_ALCHEMY_API_KEY=YOUR_ALCHEMY_API_KEY_HERE
VITE_ALCHEMY_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_API_KEY_HERE

# Smart Contract Configuration
VITE_CONTRACT_ADDRESS=YOUR_DEPLOYED_CONTRACT_ADDRESS_HERE

# IPFS Storage Configuration
VITE_NFT_STORAGE_API_KEY=YOUR_NFT_STORAGE_API_KEY_HERE

# WalletConnect Configuration
VITE_WALLETCONNECT_PROJECT_ID=YOUR_WALLETCONNECT_PROJECT_ID_HERE

# Optional: Custom RPC endpoints
VITE_CUSTOM_RPC_URL=https://your-custom-rpc-endpoint.com

# Development Configuration
VITE_DEV_MODE=true
VITE_DEBUG=false

# Security Configuration
VITE_ENABLE_TESTNET_ONLY=true
VITE_MAX_FILE_SIZE=10485760  # 10MB in bytes
VITE_ALLOWED_FILE_TYPES=image/png,image/jpeg,image/gif,image/webp

# Platform Configuration
VITE_PLATFORM_FEE_PERCENTAGE=5
VITE_MAX_ROYALTY_PERCENTAGE=20
VITE_REGISTRATION_FEE_ETH=0.01
